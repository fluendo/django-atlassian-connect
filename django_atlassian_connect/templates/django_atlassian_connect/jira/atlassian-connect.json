{
    "name": "{{ name }}",
    "description": "{{ description }}",
    "key": "{{ key }}",
    "baseUrl": "{{ base_url }}",
    "vendor": {
        "name": "{{ vendor_name }}",
        "url": "{{ vendor_url }}"
    },
    "authentication": {
        "type": "jwt"
    },
    "apiVersion": 1,
    "lifecycle":
        {% block lifecycle %}
        {
            "installed": "{% url 'django-atlassian-connect-installed' %}"
        },
        {% endblock %}
    "scopes": 
        {% block scopes %}
        [
          {% for s in scopes %}
            "{{ s }}"
            {% if not forloop.last %},{% endif %}
          {% endfor %}
        ],
        {% endblock %}
    "modules":
        {% block modules %}
        {{ modules|safe }}
        {% endblock %}
}
